<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>fullcensus</h:title><model odk:xforms-version="1.0.0"><instance><data id="fullcensus" version="2021071101"><start_time/><end_time/><device_id/><devicephonenum/><username/><have_wid/><wid_manual/><wid_qr/><wid/><country/><region/><district/><ward/><village/><hamlet/><hamlet_id/><household_id/><n_members/><note_n_members/><group_individual><person_id/><sex/><first_name/><last_name/><dob/><note_person_details/><some_number/></group_individual><meta><instanceID/><instanceName/></meta></data></instance><instance id="households_data" src="jr://file-csv/households_data.csv"/><instance id="people_data" src="jr://file-csv/people_data.csv"/><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start_time" type="dateTime"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end_time" type="dateTime"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/device_id" type="string"/><bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/devicephonenum" type="string"/><bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username" type="string"/><bind nodeset="/data/have_wid" type="string"/><bind nodeset="/data/wid_manual" relevant=" /data/have_wid ='No'" type="int"/><bind nodeset="/data/wid_qr" relevant=" /data/have_wid ='Yes'" type="barcode"/><bind calculate="if( /data/have_wid ='Yes', /data/wid_qr ,  /data/wid_manual )" nodeset="/data/wid" type="string"/><bind nodeset="/data/country" type="string"/><bind nodeset="/data/region" type="string"/><bind nodeset="/data/district" type="string"/><bind nodeset="/data/ward" type="string"/><bind nodeset="/data/village" type="string"/><bind nodeset="/data/hamlet" type="string"/><bind nodeset="/data/hamlet_id" type="string"/><bind nodeset="/data/household_id" type="string"/><bind calculate="pulldata('households_data', 'n_members', 'household_id',  /data/household_id )" nodeset="/data/n_members" type="string"/><bind nodeset="/data/note_n_members" readonly="true()" type="string"/><bind nodeset="/data/group_individual/person_id" type="string"/><bind calculate="pulldata('people_data', 'sex', 'person_id',  /data/group_individual/person_id )" nodeset="/data/group_individual/sex" type="string"/><bind calculate="pulldata('people_data', 'first_name', 'person_id',  /data/group_individual/person_id )" nodeset="/data/group_individual/first_name" type="string"/><bind calculate="pulldata('people_data', 'last_name', 'person_id',  /data/group_individual/person_id )" nodeset="/data/group_individual/last_name" type="string"/><bind calculate="pulldata('people_data', 'dob', 'person_id',  /data/group_individual/person_id )" nodeset="/data/group_individual/dob" type="string"/><bind nodeset="/data/group_individual/note_person_details" readonly="true()" type="string"/><bind nodeset="/data/group_individual/some_number" type="int"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/><bind calculate="concat('fullcensus_',  /data/household_id ,'_', /data/start_time )" nodeset="/data/meta/instanceName" type="string"/></model></h:head><h:body class="pages"><select1 ref="/data/have_wid"><label>Do you have a QR code with your worker ID?</label><item><label>Yes</label><value>1</value></item><item><label>No</label><value>2</value></item></select1><input ref="/data/wid_manual"><label>Manually write your 3 digit worker ID here</label></input><input ref="/data/wid_qr"><label>Scan your worker ID QR code</label></input><select1 appearance="search('locations_data')" ref="/data/country"><label>Select the country</label><item><label>country</label><value>country</value></item></select1><select1 appearance="search('locations_data', 'matches', 'country',  /data/country )" ref="/data/region"><label>Select the region</label><item><label>region</label><value>region</value></item></select1><select1 appearance="search('locations_data', 'matches', 'region',  /data/region )" ref="/data/district"><label>Select the district</label><item><label>district</label><value>district</value></item></select1><select1 appearance="search('locations_data', 'matches', 'district',  /data/district )" ref="/data/ward"><label>Select the ward</label><item><label>ward</label><value>ward</value></item></select1><select1 appearance="search('locations_data', 'matches', 'ward',  /data/ward )" ref="/data/village"><label>Select the village</label><item><label>village</label><value>village</value></item></select1><select1 appearance="search('locations_data', 'matches', 'village',  /data/village )" ref="/data/hamlet"><label>Select the hamlet</label><item><label>hamlet</label><value>hamlet</value></item></select1><select1 appearance="search('locations_data', 'matches', 'hamlet',  /data/hamlet )" ref="/data/hamlet_id"><label>Confirm the hamlet ID</label><item><label>hamlet_id</label><value>hamlet_id</value></item></select1><select1 appearance="search('households_data', 'matches', 'hamlet_id',  /data/hamlet_id )" ref="/data/household_id"><label>Select the household</label><item><label>household_id</label><value>household_id</value></item></select1><input ref="/data/note_n_members"><label>The number of total members is <output value=" /data/n_members "/></label></input><group ref="/data/group_individual"><select1 appearance="search('people_data', 'matches', 'household_id',  /data/household_id )" ref="/data/group_individual/person_id"><label>Select the person</label><item><label>person_id</label><value>person_id</value></item></select1><input ref="/data/group_individual/note_person_details"><label>Make sure that the following is correct:
Person ID: <output value=" /data/group_individual/person_id "/>
First name: <output value=" /data/group_individual/first_name "/>
Last name: <output value=" /data/group_individual/last_name "/>
Birthday: <output value=" /data/group_individual/dob "/></label></input><input ref="/data/group_individual/some_number"><label>Enter some number</label></input></group></h:body></h:html>